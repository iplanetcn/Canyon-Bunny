<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE module PUBLIC "-//Google Inc.//DTD Google Web Toolkit trunk//EN"
        "http://gwtproject.org/doctype/2.8.0/gwt-module.dtd">
<module rename-to="html">
    <inherits name='com.badlogic.gdx.backends.gdx_backends_gwt'/>

    <inherits name='CanyonBunnyMain'/>
    <entry-point class='com.tabcompany.libgdx.canyonbunny.client.HtmlLauncher'/>
    <set-configuration-property name='xsiframe.failIfScriptTag' value='FALSE'/>
    <set-configuration-property name="gdx.assetpath" value="../android/assets"/>

    <extend-configuration-property
            name="gdx.reflect.include"
            value="com.badlogic.gdx.scenes.scene2d.ui"
    />
    <extend-configuration-property
            name="gdx.reflect.include"
            value="com.badlogic.gdx.utils"/>
</module>
